<div class="col-md-3">
<h2>Tickets</h2>
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Number</th>
      </tr>
    </thead>

    <tbody>
      <% @tickets.each do |ticket| %>
        <tr>
          <td><%= link_to ticket.number %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
